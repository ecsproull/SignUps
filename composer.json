{
    "name": "ecsproull/sign-ups",
    "description": "A signup plugin used by the Sun City West Wood Club",
    "type": "project",
    "require": {
        "twilio/sdk": "^7.13",
        "sendgrid/sendgrid": "^8.1",
        "stripe/stripe-php": "^13.11"
    },
    "autoload": {
        "psr-4": {
            "Ecsproull\\SignUps\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ecsproull",
            "email": "ecsproull@outlook.com"
        }
    ],
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0",
        "phpcompatibility/phpcompatibility-wp": "*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
